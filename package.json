    "/lib",
    "/cloud-init",
    "index.*"
  ],
  "homepage": "https://github.com/zhifanz/fanqiang#readme",
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@types/fs-extra": "^9.0.12",
    "@types/mocha": "^8.2.3",
    "@types/node": "^14.14.31",
