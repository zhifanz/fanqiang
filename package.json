    "eslint-plugin-promise": "^5.1.0",
    "espower-typescript": "^10.0.0",
    "husky": "^7.0.0",
    "mocha": "^9.0.2",
    "npm-run-all": "^4.1.5",
    "power-assert": "^1.6.1",
    "prettier": "2.3.2",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.4.4",
    "ts-node": "^8.10.2",
    "typescript": "^4.3.5"
